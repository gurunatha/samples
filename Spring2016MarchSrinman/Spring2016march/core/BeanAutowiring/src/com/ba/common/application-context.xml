<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="robot" class="com.ba.beans.Robot" autowire="constructor" />
	<bean id="chip1" class="com.ba.beans.Chip">
		<property name="id" value="3" />
		<property name="type" value="quad-core" />
	</bean>
	<bean id="sensor1" class="com.ba.beans.Sensor">
		<property name="id" value="32" />
		<property name="manufacturer" value="intel" />
	</bean>

</beans>
